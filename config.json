{
	"name": "HTN API",

	"namespace": "Stanford\\HTNapi",

	"description": "A standalone em for API connections",

	"documentation": "README.md",

	"authors": [
		{
			"name": "Andrew Martin",
			"email": "andy123@stanford.edu",
			"institution": "Stanford University"
		},
		{
			"name": "Irvin Szeto",
			"email": "irvins@stanford.edu",
			"institution": "Stanford University"
		}
	],

	"permissions": [
		"redcap_module_system_enable",
		"redcap_module_project_enable",
		"redcap_module_save_configuration"
	],
	
	"enable-every-page-hooks-on-system-pages": false,	

	"links": {
		"project": [
			{
		        "name": "HTN api Instructions",
		        "icon": "gear",
		        "url" : "pages/instructions.php",
		        "show-header-and-footer": false
			 },
			 {
		        "name": "HTN - Omeron Oauth",
		        "icon": "gear",
		        "url" : "pages/oauth.php",
		        "show-header-and-footer": false
			 }
		],
		"control-center": [

		]
	},

	"no-auth-pages": [
		"pages/redirect",
		"pages/oauth"
	],

	"project-settings": [
		{
			"key": "omron-client-id",
			"name": "<b>Omron Client Id</b> <br> <i>Different for staging vs prod</i>",
			"required": false,
			"type": "text"
		},
		{
			"key": "omron-client-secret",
			"name": "<b>Omron Client Secret</b> <br> <i>Different for staging vs prod</i>",
			"required": false,
			"type": "text"
		},
		{
			"key": "omron-auth-url",
			"name": "<b>Omron Auth URL</b> <br> <i>Different for staging vs prod</i>",
			"required": false,
			"type": "text"
		},
		{
			"key": "omron-auth-scope",
			"name": "<b>Omron Scope of Access</b> <br> <i>seperated by space, see documentation for available data types</i>",
			"required": false,
			"type": "text"
		},
		{
			"key": "omron-postback",
			"name": "<b>Omron Postback URL</b> <br> <i>Will ideally be dynamic/automatic based on current project, but Omron requires a whitelist so for now use the URL from Production Project : <strong>21382</strong></i>",
			"required": false,
			"type": "text"
		},
		{
	      "key": "enable-project-debug-logging",
	      "name": "<b>Enable Debug Logging</b>",
	      "required": false,
	      "type": "checkbox"
	    }
	],


	"system-settings": [
		{
			"key": "enable-system-debug-logging",
			"name": "<b>Enable Debug Logging (system-wide)</b><i>(optional)</i> Requires installation and configuration of emLogger",
			"required": false,
			"type": "checkbox"
		}
	],

	"compatibility": {
		"php-version-min": "",
		"php-version-max": "",
		"redcap-version-min": "",
		"redcap-version-max": ""
	}
}
